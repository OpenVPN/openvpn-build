<Wix xmlns="http://schemas.microsoft.com/wix/2006/wi">
    <Fragment>
        <!--
            Artwork
        -->
        <Binary Id="info.ico"       SourceFile="artwork\info.ico"/>
        <Binary Id="folder_new.ico" SourceFile="artwork\folder_new.ico"/>
        <Binary Id="folder_up.ico"  SourceFile="artwork\folder_up.ico"/>
        <Binary Id="header.bmp"     SourceFile="artwork\header.bmp"/>
        <Binary Id="warning.ico"    SourceFile="artwork\warning.ico"/>


        <!--
            Text styles
        -->
        <UI>
            <TextStyle Id="DefaultUIFontXP"    FaceName="Tahoma"   Size="8"/>
            <TextStyle Id="DefaultUIFontVista" FaceName="Segoe UI" Size="9"/>
            <TextStyle Id="DlgStrongFontXP"    FaceName="Tahoma"   Size="8"  Bold="yes"/>
            <TextStyle Id="DlgStrongFontVista" FaceName="Segoe UI" Size="9"  Bold="yes"/>
            <TextStyle Id="DlgTitleFontXP"     FaceName="Tahoma"   Size="10" Bold="yes"/>
            <TextStyle Id="DlgTitleFontVista"  FaceName="Segoe UI" Size="11" Bold="yes"/>
        </UI>

        <Property Id="DefaultUIFont" Value="DefaultUIFontXP"/>
        <Property Id="DlgStrongFont" Value="{\DlgStrongFontXP}"/>
        <Property Id="DlgTitleFont"  Value="{\DlgTitleFontXP}"/>

        <CustomAction Id="SetDefaultUIFontVista" Property="DefaultUIFont" Value="DefaultUIFontVista"/>
        <CustomAction Id="SetDlgStrongFontVista" Property="DlgStrongFont" Value="{\DlgStrongFontVista}"/>
        <CustomAction Id="SetDlgTitleFontVista"  Property="DlgTitleFont"  Value="{\DlgTitleFontVista}"/>

        <InstallUISequence>
            <Custom Action="SetDefaultUIFontVista" Before="PreparePage">VersionNT&gt;=600</Custom>
            <Custom Action="SetDlgStrongFontVista" Before="PreparePage">VersionNT&gt;=600</Custom>
            <Custom Action="SetDlgTitleFontVista"  Before="PreparePage">VersionNT&gt;=600</Custom>
        </InstallUISequence>

        <AdminUISequence>
            <Custom Action="SetDefaultUIFontVista" Before="PreparePage">VersionNT&gt;=600</Custom>
            <Custom Action="SetDlgStrongFontVista" Before="PreparePage">VersionNT&gt;=600</Custom>
            <Custom Action="SetDlgTitleFontVista"  Before="PreparePage">VersionNT&gt;=600</Custom>
        </AdminUISequence>


        <!--
            Properties
        -->
        <Property Id="Setup" Value="Setup"/>


        <!--
            Prepare page
        -->
        <UI>
            <Dialog Id="PreparePage" X="50" Y="50" Width="400" Height="300" Title="[Setup] [SimpleProductName] [DisplayVersion]" Modeless="yes" NoMinimize="yes">
                <Control Id="Title" Type="Text" X="15" Y="15" Width="320" Height="15" Text="[DlgTitleFont]Preparing [SimpleProductName] installation. Please wait..." TabSkip="no" Transparent="yes" NoPrefix="yes"/>
                <Control Id="ProgressBar" Type="ProgressBar" X="30" Y="155" Width="340" Height="15" Text="Progress done" TabSkip="no" Disabled="yes" ProgressBlocks="yes">
                    <Subscribe Event="SetProgress" Attribute="Progress"/>
                </Control>
                <Control Id="ActionText" Type="Text" X="30" Y="175" Width="340" Height="15" TabSkip="no" Transparent="yes" NoPrefix="yes">
                    <Subscribe Event="ActionText" Attribute="Text"/>
                </Control>
                <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="400" Height="45" Text="header.bmp" TabSkip="no" Default="yes" Cancel="yes" Disabled="yes">
                    <Publish Event="SpawnDialog" Value="CancelDialog">1</Publish>
                </Control>
            </Dialog>
        </UI>


        <!--
            License agreement page
        -->
        <UI>
            <Dialog Id="LicenseAgreementPage" X="50" Y="50" Width="400" Height="300" Title="[Setup] [SimpleProductName] [DisplayVersion]" NoMinimize="yes">
                <Control Id="Title" Type="Text" X="15" Y="15" Width="320" Height="15" Text="[DlgTitleFont]Please read the following general terms carefully." TabSkip="no" Transparent="yes" NoPrefix="yes"/>
                <Control Id="AgreementText" Type="ScrollableText" X="15" Y="60" Width="370" Height="195" TabSkip="no" Sunken="yes">
                    <Text SourceFile="doc\LicenseAgreement.rtf"/>
                </Control>
                <Control Id="IAgree" Type="CheckBox" X="15" Y="265" Width="300" Height="15" Text="I &amp;accept the General Terms." TabSkip="no" Integer="yes" Property="IAgree" CheckBoxValue="1"/>
                <Control Id="Continue" Type="PushButton" X="325" Y="265" Width="60" Height="20" Text="&amp;Continue" TabSkip="no" Default="yes">
                    <Condition Action="disable">NOT IAgree</Condition>
                    <Condition Action="enable">IAgree</Condition>
                    <Publish Property="WaitMessage" Value="Determining required space for installation...">1</Publish>
                    <Publish Event="SpawnWaitDialog" Value="WaitDialog">CostingComplete=1</Publish>
                    <Publish Event="EndDialog" Value="Return">IAgree</Publish>
                </Control>
                <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="400" Height="45" Text="header.bmp" TabSkip="no" Cancel="yes" Disabled="yes">
                    <Publish Event="SpawnDialog" Value="CancelDialog">1</Publish>
                </Control>
            </Dialog>
        </UI>


        <!--
            Setup type page
        -->
        <UI>
            <Dialog Id="SetupTypePage" X="50" Y="50" Width="400" Height="300" Title="[Setup] [SimpleProductName] [DisplayVersion]" NoMinimize="yes">
                <Control Id="Title" Type="Text" X="15" Y="15" Width="320" Height="15" Text="[DlgTitleFont]Choose setup type." TabSkip="no" Transparent="yes" NoPrefix="yes"/>
                <Control Id="Install" Type="PushButton" X="135" Y="130" Width="130" Height="40" Text="[DlgTitleFont][ButtonTextInstall] " ToolTip="Installs the default features. (Recommended)" TabSkip="no" Default="yes" ElevationShield="yes">
                    <Publish Property="InstallMode" Value="Typical">1</Publish>
                    <Publish Property="WaitMessage" Value="Determining required space for installation...">1</Publish>
                    <Publish Event="SpawnWaitDialog" Value="WaitDialog">CostingComplete=1</Publish>
                    <Publish Event="EndDialog" Value="Return">OutOfDiskSpace&lt;&gt;1</Publish>
                    <Publish Event="SpawnDialog" Value="OutOfDiskRBPage">OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND (PROMPTROLLBACKCOST="P" OR NOT PROMPTROLLBACKCOST)</Publish>
                    <Publish Event="EndDialog" Value="Return">OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND PROMPTROLLBACKCOST="D"</Publish>
                    <Publish Event="EnableRollback" Value="False">OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND PROMPTROLLBACKCOST="D"</Publish>
                    <Publish Event="SpawnDialog" Value="OutOfDiskPage">(OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=1) OR (OutOfDiskSpace=1 AND PROMPTROLLBACKCOST="F")</Publish>
                </Control>
                <Control Id="Customize" Type="PushButton" X="165" Y="190" Width="70" Height="25" Text="C&amp;ustomize" ToolTip="Allows changing the way features are installed." TabSkip="no">
                    <Publish Property="InstallMode" Value="Custom">1</Publish>
                    <Publish Property="WaitMessage" Value="Determining required space for installation...">1</Publish>
                    <Publish Event="SpawnWaitDialog" Value="WaitDialog">CostingComplete=1</Publish>
                    <Publish Event="NewDialog" Value="CustomizePage">1</Publish>
                </Control>
                <Control Id="ActiveAdaptersIcon" Type="Icon" X="15" Y="234" Width="24"  Height="24" Hidden="yes" TabSkip="no" Disabled="yes" Text="warning.ico" ToolTip="Exclamation icon">
                    <Condition Action="show"><![CDATA[ACTIVETAPWINDOWS6ADAPTERS OR ACTIVEWINTUNADAPTERS]]></Condition>
                </Control>
                <Control Id="ActiveAdaptersLabel" Type="Text" X="44" Y="234" Width="341" Height="15" Hidden="yes" NoPrefix="yes" Transparent="yes" Text="[DlgStrongFont]Active OpenVPN connection(s) detected.">
                    <Condition Action="show"><![CDATA[ACTIVETAPWINDOWS6ADAPTERS OR ACTIVEWINTUNADAPTERS]]></Condition>
                </Control>
                <Control Id="ActiveAdaptersText" Type="Text" X="44" Y="249" Width="341" Height="36" Hidden="yes" NoPrefix="yes" Transparent="yes" Text="This setup will terminate your OpenVPN connection. If you are performing this update remotely, you will get disconnected.">
                    <Condition Action="show"><![CDATA[ACTIVETAPWINDOWS6ADAPTERS OR ACTIVEWINTUNADAPTERS]]></Condition>
                </Control>
                <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="400" Height="45" Text="header.bmp" TabSkip="no" Cancel="yes" Disabled="yes">
                    <Publish Event="SpawnDialog" Value="CancelDialog">1</Publish>
                </Control>
            </Dialog>
        </UI>

        <Property Id="ButtonTextInstall" Value="&amp;Install Now"/>
        <Property Id="InstallMode" Value="Typical"/>


        <!--
            Resume page
        -->
        <UI>
            <Dialog Id="ResumePage" X="50" Y="50" Width="400" Height="300" Title="[Setup] [SimpleProductName] [DisplayVersion]" NoMinimize="yes">
                <Control Id="Title" Type="Text" X="15" Y="15" Width="320" Height="15" Text="[DlgTitleFont]Click &quot;Next&quot; to resume [SimpleProductName] installation." TabSkip="no" Transparent="yes" NoPrefix="yes"/>
                <Control Id="Continue" Type="PushButton" X="325" Y="265" Width="60" Height="20" Text="&amp;Continue" TabSkip="no" Default="yes">
                    <Publish Property="WaitMessage" Value="Determining required space for installation...">1</Publish>
                    <Publish Event="SpawnWaitDialog" Value="WaitDialog">CostingComplete=1</Publish>
                    <Publish Event="EndDialog" Value="Return">OutOfDiskSpace&lt;&gt;1</Publish>
                    <Publish Event="SpawnDialog" Value="OutOfDiskRBPage">OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND (PROMPTROLLBACKCOST="P" OR NOT PROMPTROLLBACKCOST)</Publish>
                    <Publish Event="EndDialog" Value="Return">OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND PROMPTROLLBACKCOST="D"</Publish>
                    <Publish Event="EnableRollback" Value="False">OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND PROMPTROLLBACKCOST="D"</Publish>
                    <Publish Event="SpawnDialog" Value="OutOfDiskPage">(OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=1) OR (OutOfDiskSpace=1 AND PROMPTROLLBACKCOST="F")</Publish>
                </Control>
                <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="400" Height="45" Text="header.bmp" TabSkip="no" Cancel="yes" Disabled="yes">
                    <Publish Event="SpawnDialog" Value="CancelDialog">1</Publish>
                </Control>
            </Dialog>
        </UI>


        <!--
            Maintenance page
        -->
        <UI>
            <Dialog Id="MaintenanceTypePage" X="50" Y="50" Width="400" Height="300" Title="[Setup] [SimpleProductName] [DisplayVersion]" NoMinimize="yes">
                <Control Id="Title" Type="Text" X="15" Y="15" Width="320" Height="15" Text="[DlgTitleFont][SimpleProductName] Maintenance" TabSkip="no" Transparent="yes" NoPrefix="yes"/>
                <Control Id="Text" Type="Text" X="15" Y="60" Width="370" Height="24" Text="Maintenance allows you to change the way features are installed on your computer or even to remove [SimpleProductName] from your computer." TabSkip="no" Transparent="yes" NoPrefix="yes"/>
                <Control Id="Buttons" Type="RadioButtonGroup" X="45" Y="99" Width="310" Height="117" Property="InstallModeRB" TabSkip="no"/>
                <Control Id="Continue" Type="PushButton" X="325" Y="265" Width="60" Height="20" Text="&amp;Continue" TabSkip="no" Default="yes">
                    <Condition Action="disable">InstallModeRB&lt;&gt;"Remove"</Condition>
                    <Condition Action="enable">InstallModeRB="Remove"</Condition>
                    <Publish Property="InstallMode" Value="[InstallModeRB]">1</Publish>
                    <Publish Event="ReinstallMode" Value="ecmus">InstallMode="Repair" AND OutOfDiskSpace&lt;&gt;1</Publish>
                    <Publish Event="Remove" Value="All">InstallMode="Remove" AND OutOfDiskSpace&lt;&gt;1</Publish>
                    <Publish Event="Reinstall" Value="All">InstallMode="Repair" AND OutOfDiskSpace&lt;&gt;1</Publish>
                    <Publish Event="EndDialog" Value="Return">InstallMode="Repair" AND OutOfDiskSpace&lt;&gt;1</Publish>
                    <Publish Property="ButtonTextInstall" Value="&amp;Modify now">InstallMode="Change"</Publish>
                    <Publish Property="TitleCustomize" Value="Installed Features Selection">InstallMode="Change"</Publish>
                    <Publish Property="Progress1" Value="Modifying">InstallMode="Change"</Publish>
                    <Publish Property="Progress2" Value="Modification">InstallMode="Change"</Publish>
                    <Publish Property="Progress1" Value="Repairing">InstallMode="Repair"</Publish>
                    <Publish Property="Progress2" Value="Repair">InstallMode="Repair"</Publish>
                    <Publish Property="Progress1" Value="Removing">InstallMode="Remove"</Publish>
                    <Publish Property="Progress2" Value="Removal">InstallMode="Remove"</Publish>
                    <Publish Event="EndDialog" Value="Return">InstallMode="Remove" AND OutOfDiskSpace&lt;&gt;1</Publish>
                    <Publish Event="NewDialog" Value="CustomizePage">InstallMode="Change"</Publish>
                    <Publish Event="SpawnDialog" Value="OutOfDiskRBPage">InstallMode="Repair" AND OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND (PROMPTROLLBACKCOST="P" OR NOT PROMPTROLLBACKCOST)</Publish>
                    <Publish Event="EndDialog" Value="Return">InstallMode="Repair" AND OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND PROMPTROLLBACKCOST="D"</Publish>
                    <Publish Event="SpawnDialog" Value="OutOfDiskRBPage">InstallMode="Remove" AND OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND (PROMPTROLLBACKCOST="P" OR NOT PROMPTROLLBACKCOST)</Publish>
                    <Publish Event="EnableRollback" Value="False">InstallMode="Repair" AND OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND PROMPTROLLBACKCOST="D"</Publish>
                    <Publish Event="EndDialog" Value="Return">InstallMode="Remove" AND OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND PROMPTROLLBACKCOST="D"</Publish>
                    <Publish Event="EnableRollback" Value="False">InstallMode="Remove" AND OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND PROMPTROLLBACKCOST="D"</Publish>
                    <Publish Event="SpawnDialog" Value="OutOfDiskPage">InstallMode="Repair" AND ((OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=1) OR (OutOfDiskSpace=1 AND PROMPTROLLBACKCOST="F"))</Publish>
                    <Publish Event="SpawnDialog" Value="OutOfDiskPage">InstallMode="Remove" AND ((OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=1) OR (OutOfDiskSpace=1 AND PROMPTROLLBACKCOST="F"))</Publish>
                </Control>
                <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="400" Height="45" Text="header.bmp" TabSkip="no" Cancel="yes" Disabled="yes">
                    <Publish Event="SpawnDialog" Value="CancelDialog">1</Publish>
                </Control>
            </Dialog>

            <RadioButtonGroup Property="InstallModeRB">
                <RadioButton Height="15" Text="&amp;Modify" ToolTip="Allows changing which features are installed." Value="Change" Width="370" X="0" Y="0"/>
                <RadioButton Height="15" Text="&amp;Repair" ToolTip="Repairs errors in the current installation state." Value="Repair" Width="370" X="0" Y="20"/>
                <RadioButton Height="15" Text="&amp;Remove" ToolTip="Removes the program from your computer." Value="Remove" Width="370" X="0" Y="40"/>
            </RadioButtonGroup>
        </UI>

        <Property Id="InstallModeRB" Value="Remove"/>


        <!--
            Administrative install page
        -->
        <UI>
            <Dialog Id="AdminInstallPointPage" X="50" Y="50" Width="400" Height="300" Title="[Setup] [SimpleProductName] [DisplayVersion]" NoMinimize="yes">
                <Control Id="Title" Type="Text" X="15" Y="15" Width="320" Height="15" Text="[DlgTitleFont][SimpleProductName] Administrative Install Image Preparation" TabSkip="no" Transparent="yes" NoPrefix="yes"/>
                <Control Id="Text" Type="Text" X="15" Y="60" Width="370" Height="24" Text="You are about to prepare installation image for [SimpleProductName], to allow easier group deployment." TabSkip="no"/>
                <Control Id="PathEditLabel" Type="Text" X="15" Y="94" Width="370" Height="12" Text="&amp;Enter desired folder or click &quot;Browse&quot;:" TabSkip="no"/>
                <Control Id="PathEdit" Type="PathEdit" X="15" Y="106" Width="370" Height="18" Property="TARGETDIR" TabSkip="no"/>
                <Control Id="Browse" Type="PushButton" X="325" Y="129" Width="60" Height="20" Text="Br&amp;owse" TabSkip="no">
                    <Publish Event="SpawnDialog" Value="BrowseAdminPage">1</Publish>
                </Control>
                <Control Id="Prepare" Type="PushButton" X="140" Y="245" Width="120" Height="40" Text="[DlgStrongFont]&amp;Prepare Now" TabSkip="no" Default="yes">
                    <Publish Event="SetTargetPath" Value="TARGETDIR">1</Publish>
                    <Publish Event="EndDialog" Value="Return">OutOfDiskSpace&lt;&gt;1</Publish>
                    <Publish Event="SpawnDialog" Value="OutOfDiskRBPage">OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND (PROMPTROLLBACKCOST="P" OR NOT PROMPTROLLBACKCOST)</Publish>
                    <Publish Event="EndDialog" Value="Return">OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND PROMPTROLLBACKCOST="D"</Publish>
                    <Publish Event="EnableRollback" Value="False">OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND PROMPTROLLBACKCOST="D"</Publish>
                    <Publish Event="SpawnDialog" Value="OutOfDiskPage">(OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=1) OR (OutOfDiskSpace=1 AND PROMPTROLLBACKCOST="F")</Publish>
                </Control>
                <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="400" Height="45" Text="header.bmp" TabSkip="no" Cancel="yes" Disabled="yes">
                    <Publish Event="SpawnDialog" Value="CancelDialog">1</Publish>
                </Control>
            </Dialog>

            <Dialog Id="BrowseAdminPage" X="50" Y="50" Width="400" Height="300" Title="[Setup] [SimpleProductName] [DisplayVersion]" NoMinimize="yes">
                <Control Id="Title" Type="Text" X="15" Y="15" Width="320" Height="15" Text="[DlgTitleFont]Change current administrative installation folder." TabSkip="no" Transparent="yes" NoPrefix="yes"/>
                <Control Id="DirectoryComboLabel" Type="Text" X="15" Y="60" Width="40" Height="15" Text="&amp;Look in:" TabSkip="no"/>
                <Control Id="DirectoryCombo" Type="DirectoryCombo" X="55" Y="60" Width="280" Height="80" Property="TARGETDIR" TabSkip="no" Removable="yes" Fixed="yes" Remote="yes">
                    <Subscribe Event="IgnoreChange" Attribute="IgnoreChange"/>
                </Control>
                <Control Id="Up" Type="PushButton" X="340" Y="60" Width="18" Height="18" Text="folder_up.ico" ToolTip="Up One Level" TabSkip="no" Icon="yes">
                    <Publish Event="DirectoryListUp" Value="0">1</Publish>
                </Control>
                <Control Id="NewFolder" Type="PushButton" X="365" Y="60" Width="18" Height="18" Text="folder_new.ico" ToolTip="Create A New Folder" TabSkip="no" Icon="yes">
                    <Publish Event="DirectoryListNew" Value="0">1</Publish>
                </Control>
                <Control Id="DirectoryList" Type="DirectoryList" X="15" Y="83" Width="370" Height="149" Property="TARGETDIR" TabSkip="no" Sunken="yes"/>
                <Control Id="PathEditLabel" Type="Text" X="15" Y="237" Width="50" Height="15" Text="&amp;Folder:" TabSkip="no"/>
                <Control Id="PathEdit" Type="PathEdit" X="65" Y="237" Width="320" Height="18" Property="TARGETDIR" TabSkip="no"/>
                <Control Id="OK" Type="PushButton" X="260" Y="265" Width="60" Height="20" Text="OK" TabSkip="no" Default="yes">
                    <Publish Property="InstallMode" Value="Server Image">1</Publish>
                    <Publish Event="SetTargetPath" Value="TARGETDIR">1</Publish>
                    <Publish Event="EndDialog" Value="Return">1</Publish>
                </Control>
                <Control Id="Cancel" Type="PushButton" X="325" Y="265" Width="60" Height="20" Text="Cancel" TabSkip="no" Cancel="yes">
                    <Publish Event="Reset" Value="0">1</Publish>
                    <Publish Event="EndDialog" Value="Return">1</Publish>
                </Control>
                <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="400" Height="45" Text="header.bmp" TabSkip="no" Disabled="yes"/>
            </Dialog>
        </UI>


        <!--
            Customize page
        -->
        <UI>
            <Dialog Id="CustomizePage" X="50" Y="50" Width="400" Height="300" Title="[Setup] [SimpleProductName] [DisplayVersion]" NoMinimize="yes" TrackDiskSpace="yes">
                <Control Id="Title" Type="Text" X="15" Y="15" Width="320" Height="15" Text="[DlgTitleFont][TitleCustomize] " TabSkip="no" Transparent="yes" NoPrefix="yes"/>
                <Control Id="TreeLabel" Type="Text" X="15" Y="60" Width="370" Height="15" Text="Click on the icons in the tree below to change the features to be installed:" TabSkip="no"/>
                <Control Id="Tree" Type="SelectionTree" X="15" Y="75" Width="370" Height="93" Property="_BrowseProperty" Text="Tree of selections" TabSkip="no" Sunken="yes"/>
                <Control Id="Box" Type="GroupBox" X="15" Y="173" Width="370" Height="82" TabSkip="no" Disabled="yes"/>
                <Control Id="ItemDescription" Type="Text" X="20" Y="182" Width="360" Height="24" Text="Multiline description of the currently selected item." TabSkip="no" NoPrefix="yes">
                    <Subscribe Event="SelectionDescription" Attribute="Text"/>
                </Control>
                <Control Id="ItemSize" Type="Text" X="20" Y="206" Width="360" Height="24" Text="The size of the currently selected item." TabSkip="no" NoPrefix="yes">
                    <Subscribe Event="SelectionSize" Attribute="Text"/>
                </Control>
                <Control Id="LocationLabel" Type="Text" X="20" Y="235" Width="40" Height="15" Text="Folder:" TabSkip="no">
                    <Condition Action="disable">Installed</Condition>
                    <Subscribe Event="SelectionPathOn" Attribute="Enabled"/>
                </Control>
                <Control Id="Location" Type="Text" X="60" Y="235" Width="255" Height="15" TabSkip="no" NoPrefix="yes">
                    <Condition Action="disable">Installed</Condition>
                    <Subscribe Event="SelectionPath" Attribute="Text"/>
                    <Subscribe Event="SelectionPathOn" Attribute="Enabled"/>
                </Control>
                <Control Id="Browse" Type="PushButton" X="320" Y="230" Width="60" Height="20" Text="Br&amp;owse" TabSkip="no">
                    <Condition Action="hide">WIX_UPGRADE_DETECTED</Condition>
                    <Publish Event="SelectionBrowse" Value="BrowsePage">1</Publish>
                </Control>
                <Control Id="Reset" Type="PushButton" X="15" Y="265" Width="60" Height="20" Text="&amp;Reset" ToolTip="Resets feature selection to initial state." TabSkip="no">
                    <Publish Event="Reset" Value="0">1</Publish>
                    <Subscribe Event="SelectionNoItems" Attribute="Enabled"/>
                </Control>
                <Control Id="DiskCost" Type="PushButton" X="80" Y="265" Width="60" Height="20" Text="Disk &amp;Usage" ToolTip="Shows space requirement of selected features." TabSkip="no">
                    <Publish Event="SpawnDialog" Value="DiskCostPage">1</Publish>
                    <Subscribe Event="SelectionNoItems" Attribute="Enabled"/>
                </Control>
                <Control Id="Install" Type="PushButton" X="295" Y="265" Width="90" Height="20" Text="[ButtonTextInstall] " TabSkip="no" ElevationShield="yes">
                    <Publish Event="EndDialog" Value="Return">OutOfDiskSpace&lt;&gt;1</Publish>
                    <Publish Event="SpawnDialog" Value="OutOfDiskRBPage">OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND (PROMPTROLLBACKCOST="P" OR NOT PROMPTROLLBACKCOST)</Publish>
                    <Publish Event="EndDialog" Value="Return">OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND PROMPTROLLBACKCOST="D"</Publish>
                    <Publish Event="EnableRollback" Value="False">OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=0 AND PROMPTROLLBACKCOST="D"</Publish>
                    <Publish Event="SpawnDialog" Value="OutOfDiskPage">(OutOfDiskSpace=1 AND OutOfNoRbDiskSpace=1) OR (OutOfDiskSpace=1 AND PROMPTROLLBACKCOST="F")</Publish>
                    <Subscribe Event="SelectionNoItems" Attribute="Enabled"/>
                </Control>
                <Control Id="BannerBitmap" Type="Bitmap" X="0" Y="0" Width="400" Height="45" Text="header.bmp" TabSkip="no" Cancel="yes" Disabled="yes">
                    <Publish Event="SpawnDialog" Value="CancelDialog">1</Publish>
                </Control>
            </Dialog>

            <Dialog Id="BrowsePage" X="50" Y="50" Width="400" Height="300" Title="[Setup] [SimpleProductName] [DisplayVersion]" NoMinimize="yes">
                <Control Id="Title" Type="Text" X="15" Y="15" Width="320" Height="15" Text="[DlgTitleFont]Change current destination folder." TabSkip="no" Transparent="yes" NoPrefix="yes"/>
                <Control Id="DirectoryComboLabel" Type="Text" X="15" Y="60" Width="40" Height="15" Text="&amp;Look in:" TabSkip="no"/>
                <Control Id="DirectoryCombo" Type="DirectoryCombo" X="55" Y="60" Width="280" Height="80" Property="_BrowseProperty" TabSkip="no" Indirect="yes" Fixed="yes" Remote="yes">
                    <Subscribe Event="IgnoreChange" Attribute="IgnoreChange"/>
                </Control>
